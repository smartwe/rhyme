  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "../public/electronjs",
    "allowSyntheticDefaultImports": true,
    "rootDir": ".",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": ["es2020", "dom"],
    "noImplicitAny": true,
    "baseUrl": "."
  },
  "include": ["**/*"]
}
