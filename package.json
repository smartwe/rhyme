{
  "name": "rhyme",
  "version": "1.0.0",
  "description": "The Home of your music.",
  "main": "dist/main/bundle.js",
  "scripts": {
    "build": "webpack --config webpack/webpack.prod.js --color",
    "dev": "webpack --config webpack/webpack.dev.js --color --watch",
    "serve": "fast-live-reload dist/renderer",
    "electron": "electron .",
    "rhyme": "npm run build && concurrently npm:serve npm:dev npm:electron",
    "test:lint": "eslint . --ext .tsx,.ts,.js",
    "test:formatting": "prettier --check \"./**/*.{ts,tsx,js}\"",
    "fix:lint": "eslint . --ext .tsx,.ts,.js --fix",
    "fix:formatting": "prettier --write \"./**/*.{ts,tsx,js}\"",
    "package:snap": "electron-builder -l",
    "package:linux": "electron-builder -l",
    "package:windows": "electron-builder -w",
    "package:mac": "electron-builder -m"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rhyme-Player/Rhyme.git"
  },
  "author": "",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Rhyme-Player/Rhyme/issues"
  },
  "homepage": "https://github.com/Rhyme-Player/Rhyme#readme",
  "dependencies": {
    "cutagerjs": "^1.2.1",
    "electron-store": "^8.0.1",
    "ps-node": "0.1.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1"
  },
  "devDependencies": {
    "@types/ps-node": "^0.1.1",
    "@types/react": "17.0.33",
    "@types/react-dom": "17.0.10",
    "@types/react-redux": "^7.1.20",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "clean-terminal-webpack-plugin": "3.0.0",
    "clean-webpack-plugin": "4.0.0",
    "concurrently": "^6.3.0",
    "css-loader": "6.4.0",
    "electron": "15.3.0",
    "electron-builder": "22.13.1",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.4.3",
    "node-loader": "2.0.0",
    "node-sass": "^6.0.1",
    "prettier": "2.4.1",
    "prettier-eslint": "13.0.0",
    "terser-webpack-plugin": "5.2.4",
    "ts-loader": "9.2.6",
    "typescript": "4.4.4",
    "webpack": "5.60.0",
    "webpack-cli": "4.9.1",
    "webpack-merge": "5.8.0",
    "webpackbar": "5.0.0-3"
  }
}
